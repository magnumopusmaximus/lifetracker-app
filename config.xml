<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lifetracker.beta" version="1.3.1" android-versionCode="10035" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LifeTrackerBeta</name>
    <description>LifeTracker Beta App</description>
    <author email="glockenhammer3301+privacy@gmail.com" href="https://magnumopusmaximus.github.io/lifetracker-website/">
        LifeTracker Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="Orientation" value="portrait" />
<platform name="android">
    <preference name="android:minSdkVersion" value="24" />
    <preference name="android:targetSdkVersion" value="35" />
    <preference name="android:compileSdkVersion" value="35" />
    <icon src="www/res/android/logo.png" />
</platform>
    <engine name="cordova-android" spec="~12.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~2.4.3" />
    <plugin name="cordova-plugin-device" spec="~2.0.3" />
</widget>